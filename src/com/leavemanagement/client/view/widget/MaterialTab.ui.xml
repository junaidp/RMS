<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	
<m:MaterialRow>
<m:MaterialColumn grid="s12">
<m:MaterialTab ui:field="tab" indicatorColor="YELLOW" shadow="1" >
<m:MaterialTabItem waves="YELLOW" grid="s4" ui:field="tabPendingLeave" >
<m:MaterialLink text="Pending Leave" href="#tab1" textColor="WHITE" />
</m:MaterialTabItem>
<m:MaterialTabItem waves="YELLOW" grid="s4" visible="false" ui:field="tabLeaveHistory">
<m:MaterialLink text="Leaves History" href="#tab2" textColor="WHITE"/>
</m:MaterialTabItem>
<m:MaterialTabItem waves="YELLOW" grid="s4" visible="false" ui:field="tabEmployeeLeave">
<m:MaterialLink text="Employees Leave" href="#tab3" textColor="WHITE"/>
</m:MaterialTabItem>

<m:MaterialTabItem waves="YELLOW" grid="s4">
<m:MaterialLink text="Job Creation" href="#tab4" textColor="WHITE"/>
</m:MaterialTabItem>

<m:MaterialTabItem waves="YELLOW" grid="s4">
<m:MaterialLink text="Attributes" href="#tab5" textColor="WHITE"/>
</m:MaterialTabItem>

<m:MaterialTabItem waves="YELLOW" grid="s4">
<m:MaterialLink text="Rating" href="#tab6" textColor="WHITE"/>
</m:MaterialTabItem>

<m:MaterialTabItem waves="YELLOW" grid="s4">
<m:MaterialLink text="Time Sheet" href="#tab7" textColor="WHITE"/>
</m:MaterialTabItem>

<m:MaterialTabItem waves="YELLOW" grid="s4" visible="false" ui:field="tabTimeReport">
<m:MaterialLink text="Time Report" href="#tab8" textColor="WHITE"/>
</m:MaterialTabItem>


</m:MaterialTab>

</m:MaterialColumn>
<m:MaterialColumn m:id="tab1" grid="s12" ui:field="tab1">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab2" grid="s12" ui:field="tab2">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab3" grid="s12" ui:field="tab3">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab4" grid="s12" ui:field="tab4">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab5" grid="s12" ui:field="tab5">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab6" grid="s12" ui:field="tab6">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab7" grid="s12" ui:field="tab7">
</m:MaterialColumn>

<m:MaterialColumn m:id="tab8" grid="s12" ui:field="tab8">
</m:MaterialColumn>




</m:MaterialRow>
</g:HTMLPanel>
</ui:UiBinder> 